<?php

namespace Cabinet\MedicalBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * prisecharge
 */
class prisecharge
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $titre;

    /**
     * @var \DateTime
     */
    private $dateprise;

    /**
     * @var string
     */
    private $nom;

    /**
     * @var string
     */
    private $prenom;

    /**
     * @var \DateTime
     */
    private $datenaiss;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set titre
     *
     * @param string $titre
     * @return prisecharge
     */
    public function setTitre($titre)
    {
        $this->titre = $titre;
    
        return $this;
    }

    /**
     * Get titre
     *
     * @return string 
     */
    public function getTitre()
    {
        return $this->titre;
    }

    /**
     * Set dateprise
     *
     * @param \DateTime $dateprise
     * @return prisecharge
     */
    public function setDateprise($dateprise)
    {
        $this->dateprise = $dateprise;
    
        return $this;
    }

    /**
     * Get dateprise
     *
     * @return \DateTime 
     */
    public function getDateprise()
    {
        return $this->dateprise;
    }

    /**
     * Set nom
     *
     * @param string $nom
     * @return prisecharge
     */
    public function setNom($nom)
    {
        $this->nom = $nom;
    
        return $this;
    }

    /**
     * Get nom
     *
     * @return string 
     */
    public function getNom()
    {
        return $this->nom;
    }

    /**
     * Set prenom
     *
     * @param string $prenom
     * @return prisecharge
     */
    public function setPrenom($prenom)
    {
        $this->prenom = $prenom;
    
        return $this;
    }

    /**
     * Get prenom
     *
     * @return string 
     */
    public function getPrenom()
    {
        return $this->prenom;
    }

    /**
     * Set datenaiss
     *
     * @param \DateTime $datenaiss
     * @return prisecharge
     */
    public function setDatenaiss($datenaiss)
    {
        $this->datenaiss = $datenaiss;
    
        return $this;
    }

    /**
     * Get datenaiss
     *
     * @return \DateTime 
     */
    public function getDatenaiss()
    {
        return $this->datenaiss;
    }
    /**
     * @var \Cabinet\MedicalBundle\Entity\patient
     */
    private $pat;


    /**
     * Set pat
     *
     * @param \Cabinet\MedicalBundle\Entity\patient $pat
     * @return prisecharge
     */
    public function setPat(\Cabinet\MedicalBundle\Entity\patient $pat = null)
    {
        $this->pat = $pat;
    
        return $this;
    }

    /**
     * Get pat
     *
     * @return \Cabinet\MedicalBundle\Entity\patient 
     */
    public function getPat()
    {
        return $this->pat;
    }
}